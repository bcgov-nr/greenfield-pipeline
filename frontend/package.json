{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "quikcstart-openshift",
  "description": "Vue3 Vuetify TypeScript Starter project for Vite.",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "type": "module",
  "author": {
    "name": "Om",
    "email": "omprakash.2.mishra@gov.bc.ca"
  },
  "engines": {
    "node": ">=18.14.2",
    "npm": ">=7.24.2"
  },
  "scripts": {
    "dev": "vite --host",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "preview": "vite preview",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "core-js": "^3.30.2",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.1.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vuetify": "^3.3.2",
    "webfontloader": "^1.6.28",
    "vue-native-websocket-vue3": "^3.1.7"
  },
  "devDependencies": {
    "@babel/types": "^7.22.4",
    "@rushstack/eslint-patch": "^1.3.0",
    "@tsconfig/node-lts": "^18.12.2",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.2.5",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.31.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.41.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "latest",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "eslint-plugin-vuetify": "^2.0.1",
    "eslint-plugin-yaml": "^0.5.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^6.0.3",
    "typescript": "4.9.5",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.9",
    "vite-plugin-checker": "0.6.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.31.4",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.7.0"
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  }
}
